<title>Tabla de circuitos</title>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js">
</script>
<div>
    <link rel="stylesheet" href="../../resources/css/actualizar.css" type="text/css">
    <!--link de plataforma de donde vienen los icons-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" 
    rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" 
    crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Anton|Montserrat&display=swap" rel="stylesheet">
    
    <link href="../../resources/css/lateral.css" rel="stylesheet" type="text/css">

<div class="table-responsive" style="background-color: #fff;">
<table class="table m-0" id="TABLA" style="text-align:center;">
    <thead class="text-center bg-light">
        <tr>
            <th>ID DEL CIRCUITO</th>
            <th>NOMBRE DEL CIRCUITO</th>
            <th>CIRCUITO HABILITADO</th>
            <th>NOMBRE DEL PROVEEDOR</th>
            <th>NOMBRE DEL USUARIO</th>
        </tr>
    </thead>
    <tbody class="fw-light">
        <tr>
            <% if(datosTodos) { %>
                <% for(let i=0; i < datosTodos.length; i++) { 

                    if(datosTodos[i].cir_habilitado===1){
                        datosTodos[i].cir_habilitado='HABILITADO'
                    }else{
                        datosTodos[i].cir_habilitado='INHABILITADO'
                    }
                    %>
            <td> <%= datosTodos[i].id_cir %> </td>
            <td> <%= datosTodos[i].nombre_cir %> </td>
            <td> <%= datosTodos[i].cir_habilitado %> </td>
            <td> <%= datosTodos[i].nombre_prov %> </td>
            <td> <%= datosTodos[i].nombre_usuario %> </td>
        </tr>
        <% } %>
            <% } %>
    </tbody>
</table>
<br>
<a href="javascript:closed();"  class="btn btn-outline-secondary btn-sm" id="button-addon2">CERRAR</a>
</div>
<%- include('../includes/footer.ejs') %>
<script language="javascript" type="text/javascript"> 
    function closed() { window.open('','_parent',''); window.close(); }
    </script> 